@using Microsoft.AspNetCore.Identity
@inject SignInManager<IdentityUser> SignInManager

@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">ENUM</h1>
    <p>EEN User Management</p>
    @if (SignInManager.IsSignedIn(User))
    {
        <a class="btn btn-outline-dark" asp-area="Identity" asp-page="/Account/Manage/Index" title="Manage">Manage your account</a>
    }
    else
    {
        <a class="btn btn-outline-dark" asp-area="Identity" asp-page="/Account/Login">Login</a>
    }
</div>